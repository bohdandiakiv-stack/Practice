{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Warning"
    }
  },
  "Couchbase": {
    "Image": "couchbase/server",
    "Tag": "latest",
    "AdminUser": "Administrator",
    "AdminPassword": "password",
    "DataVolume": "couchbase-data",
    "DataPath": "/opt/couchbase/var",
    "Ports": {
      "Console": 8091,
      "Memcached": 11210,
      "Query":  8093
    }
  },
  "CouchbaseInit": {
    "Image": "alpine:latest",
    "ScriptHostPath": "init-couchbase.sh",
    "ScriptContainerPath": "/init-couchbase.sh"
  },
  "TaskManagerApi": {
    "DockerfilePath": "../",
    "DockerfileName": "TaskManager.Api/Dockerfile",
    "Port": 8080,
    "TargetPort": 8080,
    "ConnectionStrings": {
      "Couchbase": "couchbase://couchbase:11210"
    },
    "Couchbase": {
      "Username": "Administrator",
      "Password": "password",
      "BucketName": "TaskManager"
    }
  }
}
